        <li class="nav-item">
            <a href="" class="nav-link active"  style="color: rgb(10, 10, 94);">
                About
            </a>
        </li>
    
    
        
    </ul>
    
    </div>
  
  </div>
  </nav>

    <section class="h-100 h-custom">
        <div class="container py-5 h-100">
          <div class="row d-flex justify-content-center align-items-center h-100">
            <div class="col-lg-8 col-xl-6">
              <div class="card rounded-3">
                
                <div class="card-body p-4 p-md-5">
                  <h3 class="mb-4 pb-2 pb-md-0 mb-md-5 px-md-2">Select Category</h3>
      
                  <form class="px-md-2" method="post" action="/pass">
      
                    <div class="form-outline mb-4">
                      <label class="form-label" for="form3Example1q" > Password Details</label>
                      <select  name="na" class="form-control" >                       
                        <% records.forEach(function(item){ %>
                        <option value="<%=item.name %>"><%=item.name %></option>
                         <%})%>
                      </select>
                    </div>
                  </div>                      
                      <div class="form-outline mb-4">
                     <label class="form-label" for="form3Example1q"> Project Name</label>
                      <input type="text" id="form3Example1q" name="pro" class="form-control" />
                      
                    </div>
                  
                    <textarea class="form-control" id="editor" name="details" rows="5" placeholder="Enter Your Password Details"></textarea>
                   
      <br>
      
                    <button type="submit" onclick="show()" class="btn btn-primary btn-lg mb-1">ADD Password</button>
                    
      
                  </form>
      
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
    </section>
  <script>
   
                    CKEDITOR.replace( 'editor' );
  </script>  
 
